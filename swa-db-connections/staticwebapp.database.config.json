{
  "$schema": "https://github.com/Azure/data-api-builder/releases/download/v0.8.50/dab.draft.schema.json",
  "data-source": {
    "database-type": "mysql",
    "connection-string": "",
    "options": {}
  },
  "runtime": {
    "rest": {
      "enabled": true,
      "path": "/rest"
    },
    "graphql": {
      "enabled": true,
      "path": "/graphql",
      "allow-introspection": true
    },
    "host": {
      "cors": {
        "origins": [],
        "allow-credentials": false
      },
      "authentication": {
        "provider": "StaticWebApps"
      },
      "mode": "production"
    }
  },
  "entities": {}
}